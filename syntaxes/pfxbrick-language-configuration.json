{
    "comments": {
      "lineComment": "//",
      "lineCommant": "#"
    },
    "brackets": [
      ["{", "}"],
      ["[", "]"],
      ["(", ")"]
    ],
    "autoClosingPairs": [
      { "open": "{", "close": "}" },
      { "open": "[", "close": "]" },
      { "open": "(", "close": ")" },
      { "open": "\"", "close": "\"", "notIn": ["string"] }
    ],
    "autoCloseBefore": ";:.,=}])>` \n\t",
    "surroundingPairs": [
      ["{", "}"],
      ["[", "]"],
      ["(", ")"],
      ["\"", "\""]
    ],
    "folding": {
      "markers": {
        "start": "^\\s*//\\s*#?region\\b",
        "end": "^\\s*//\\s*#?endregion\\b"
      }
    }
  }
  